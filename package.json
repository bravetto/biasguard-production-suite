{
  "name": "biasguard-landing",
  "version": "1.0.0",
  "description": "BiasGuard Code landing page - Ultra-lightweight AI bias detection born from fighting for justice",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "node dev-server.js",
    "start": "npm run dev",
    "serve": "npx serve -s . -l 3000",
    "serve:python": "python3 -m http.server 3000 || python -m http.server 3000",
    "preview": "npx serve -s . -l 4173",
    "build": "echo 'No build step required for static HTML' && exit 0",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "validate": "npx html-validate index.html",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "lighthouse:mobile": "npx lighthouse http://localhost:3000 --preset=perf --form-factor=mobile --throttling-method=devtools --output=json --output-path=./lighthouse-mobile.json",
    "test": "npm run validate && npm run lighthouse",
    "test:mobile": "npm run lighthouse:mobile",
    "clean": "rm -rf .vercel lighthouse-report.json lighthouse-mobile.json",
    "setup": "echo 'Setting up BiasGuard Code development environment...' && npm install ws --save-dev",
    "precommit": "npm run validate"
  },
  "keywords": [
    "ai",
    "bias-detection",
    "machine-learning",
    "fairness",
    "justice",
    "algorithmic-bias",
    "landing-page",
    "static-site",
    "biasguard",
    "criminal-justice-reform"
  ],
  "author": {
    "name": "BiasGuards.ai",
    "url": "https://biasguards.ai",
    "email": "contact@biasguards.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biasguards/biasguard-landing.git"
  },
  "bugs": {
    "url": "https://github.com/biasguards/biasguard-landing/issues"
  },
  "homepage": "https://biasguards.ai",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "ws": "^8.14.2"
  },
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "config": {
    "domain": "biasguards.ai",
    "title": "BiasGuard Code - AI Bias Detection",
    "description": "17KB AI bias detection tool born from fighting for justice"
  },
  "funding": {
