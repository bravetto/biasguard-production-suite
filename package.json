{
  "name": "biasguard-landing",
  "version": "1.0.0",
  "description": "BiasGuard Code landing page - Lightweight AI bias detection tool supporting justice reform",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "node dev-server.js",
    "start": "npm run dev",
    "serve": "npx serve -s . -l 3009",
    "serve:python": "python3 -m http.server 3009 || python -m http.server 3009",
    "preview": "npx serve -s . -l 4173",
    "build": "echo 'Static HTML build' && exit 0",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "validate": "npx html-validate index.html",
    "lighthouse": "npx lighthouse http://localhost:3009 --output=json",
    "lighthouse:mobile": "npx lighthouse http://localhost:3009 --preset=perf --form-factor=mobile --output=json",
    "test": "npm run validate && npm run lighthouse",
    "test:mobile": "npm run lighthouse:mobile",
    "clean": "rm -rf .vercel *.json",
    "setup": "echo 'Installing dependencies...' && npm install ws --save-dev",
    "precommit": "npm run validate"
  },
  "keywords": [
    "ai",
    "bias-detection",
    "fairness",
    "justice",
    "biasguard"
  ],
  "author": {
    "name": "BiasGuards.ai",
    "url": "https://biasguards.ai",
    "email": "contact@biasguards.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biasguards/biasguard-landing.git"
  },
  "bugs": {
    "url": "https://github.com/biasguards/biasguard-landing/issues"
  },
  "homepage": "https://biasguards.ai",
  "engines": {
    "node": "^16.0.0 || ^18.0.0 || ^20.0.0"
  },
  "devDependencies": {
    "ws": "^8.18.3"
  },
  "browserslist": [
    "> 2%",
    "last 3 versions"
  ],
  "config": {
    "domain": "biasguards.ai",
    "title": "BiasGuard Code - AI Bias Detection",
    "description": "AI bias detection tool supporting justice reform"
  },
  "funding": {
    "type": "individual",
    "url": "https://buy.stripe.com/5kQeVeeX7AzbXm1GdI1A00"
  },
  "private": false
}
