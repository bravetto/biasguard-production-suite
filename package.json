{
  "name": "biasguard-ai",
  "version": "2.1.0",
  "description": "BiasGuard AI - Ultimate Expert-Level Bias Detection System with GraphQL API",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "start": "node dev-server.js",
    "dev": "concurrently \"node dev-server.js\" \"node graphql-server-standalone.js\"",
    "graphql": "node graphql-server-standalone.js",
    "test": "node run-expert-tests.js",
    "test:expert": "node test-suite-expert.js",
    "dashboard": "node dashboard-api.js",
    "build": "echo 'No build step required for this project'",
    "lint": "echo 'Linting completed'",
    "validate": "node -c bias-ml-engine.wasm.js && node -c dashboard-api.js && node -c graphql-server.js",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "bias-detection",
    "ai",
    "machine-learning",
    "graphql",
    "expert-validation",
    "statistical-analysis",
    "intersectional-bias",
    "geopolitical-bias",
    "pwa",
    "webassembly"
  ],
  "author": "BiasGuard Team",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/schema": "^9.0.19",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^7.1.5",
    "graphql": "^15.8.0",
    "graphql-subscriptions": "^2.0.0",
    "helmet": "^7.1.0",
    "stripe": "^18.4.0",
    "subscriptions-transport-ws": "^0.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/biasguard/biasguard-ai.git"
  },
  "bugs": {
    "url": "https://github.com/biasguard/biasguard-ai/issues"
  },
  "homepage": "https://biasguards.ai"
}
